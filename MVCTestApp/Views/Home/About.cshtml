@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<div>
    @helper ShowList(string[] args)
    {
        if (args.Length == 0)
        {
            <label>No elements in list</label>
        }
        else
        {
            <ul>
                @foreach (string str in args)
                {
                    <li>@str</li>
                }
            </ul>
        }
    }
</div>

@ShowList(new string[] { "JAN", "FEB", "MAR" });
